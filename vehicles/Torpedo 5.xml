	w = screen.getWidth()
	h = screen.getHeight()					
	screen.setColor(0, 0, 0)			 
	screen.drawClear()
	screen.setColor(0,255,0)
	
	local txt=""
	for i=1,32 do
		txt=txt .. string.format("%.0f:%.0f ", i, numbers[i])
	end
	
	txt=txt .. "\n"
	
	for i=1,32 do
		txt=txt .. string.format("%.0f:%s ", i, boolstr(bools[i]))
	end
	
	screen.drawTextBox(0,0,w,h,txt)
end'><pos x="0.25" y="-1"/><in1 component_id="2"/></object></c></components><components_bridge><c type="4"><object id="2"><pos x="-1.25" y="-0.75"/></object></c><c type="7"><object id="6"><pos x="2" y="-1"/><in1 component_id="3" node_index="1"/></object></c></components_bridge><groups/></group></microprocessor_definition><vp x="-9" y="-22" z="16"/><logic_slots><slot/><slot/></logic_slots></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="-9" y="-25" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="-9" y="-26" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="-9" y="-27" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="-9" y="-28" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="-9" y="-29" z="10"/></o></c><c d="rx_huge_v2"><o r="1,0,0,0,0,1,0,-1,0" sc="5"><vp x="9" y="-24" z="23"/><logic_slots><slot/><slot/><slot/><slot/><slot/><slot/><slot/><slot/><slot/><slot/></logic_slots></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="9" y="-25" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="9" y="-26" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="9" y="-27" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="9" y="-28" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="9" y="-29" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="-9" y="-30" z="10"/></o></c><c><o r="0,0,-1,1,0,0,0,-1,0" sc="6"><vp x="-8" y="-30" z="10"/></o></c><c><o r="0,0,-1,1,0,0,0,-1,0" sc="6"><vp x="-7" y="-30" z="10"/></o></c><c><o r="0,0,-1,1,0,0,0,-1,0" sc="6"><vp x="-6" y="-30" z="10"/></o></c><c><o r="0,0,-1,1,0,0,0,-1,0" sc="6"><vp x="-5" y="-30" z="10"/></o></c><c><o r="-1,0,0,0,-1,0,0,0,1" sc="6"><vp x="9" y="-30" z="10"/></o></c><c><o sc="6"><vp x="8" y="-30" z="10"/></o></c><c><o sc="6"><vp x="7" y="-30" z="10"/></o></c><c><o sc="6"><vp x="6" y="-30" z="10"/></o></c><c><o sc="6"><vp x="5" y="-30" z="10"/></o></c><c><o sc="6"><vp x="4" y="-30" z="10"/></o></c><c><o sc="6"><vp x="3" y="-30" z="10"/