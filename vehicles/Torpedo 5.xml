function gn(n)
	return input.getNumber(n)
end

function sn(n,v)
	output.setNumber(n,v)
end

function gb(n)
	return input.getBool(n)
end

function sb(n,v)
	output.setBool(n,v)
end

-- TV Guided version
--
-- Ordnance telemetry protocol (output)
-- We overlay the contact age field since I never found it to work anyway
-- These fields are optional and the booleans specify whether the piece of ordnance provides them
-- Boolean 9:  Lock ordinal: If true, Numeric 4 contains the contact ordinal of the locked or targeted contact
-- Boolean 10: FOV: If true, Number 8 contains the FOV of the weapon's sensors for drawing a targeting ring
--					on the HUD
-- Boolean 11 Range. Offer sensor rang on Bool 12 to draw a ring on the radar

-- Number 4: Ordinal of lock-on contact. In the case of stock missile radar heads, it's always the closest contact
-- Number 8: FOV so that it can be displayed on the HUD
-- Number 12: Weapon sensor/targeting range in meters
-- Number 16: Reserved for ordnance type read from hardpoint and inserted later by MCU
-- Number 32: Reserved for radar MCU to track radar display scale/radius in m

-- Protocol (input)
-- Numeric 1: Yaw command
-- Numeric 2: Pitch command
-- Numeric 32: Hardpoint #/bus address
-- Boolean 1: Launch (applied also to hardpoint)
-- Boolean 9: Enable the weapon's sensors (radar etc.) which should use the game's standard radar
-- 			protocol to report contacts on the composite-out.
-- Check gb(9) to see whether the pilot is scanning for a target.


function onTick()

	local range=inf
	local nearest=0
	local r
	local present
	
	scan=gb(9)
	
	if not scan then return end
	
	--[[
	for i=1,8,1
	do
		present = gb(i)
		sb(i,present)
		if present then
			r=gn(i*4-3)
			if r &lt; range and r &gt; 100 then -- Assume that anything inside 100m is self-detection
				range=r
				nearest=i
			end
		end
	end
	]]
	
	for i=1,32,1
	do
		sn(i,gn(i))
	end

	sb(9,false)  -- Notify we will report a target ordinal
	sb(10,false)  -- Notify we will report sensor/radar FOV
	sb(11,false) -- Notify we will provide sensor range
	
	-- sn(4,nearest)
	-- sn(8,20)	-- Roughly 20 degrees
	-- sn(12,2000) -- 2km gives about a 20 degree X/Y FOV. Should match the radar head configuration.
end

function onDraw()
